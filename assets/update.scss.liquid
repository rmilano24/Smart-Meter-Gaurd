/* ---------home12 banner v1 ------------------ */


.site-banner{
 &.banner-v1{
   .row{
     margin-left: 0px;
     margin-right: 0px;
   }
     brand
    .col-md-3 {
      padding-right: 0;
      padding-left: 0;
      overflow: hidden;
      .banner{
		.banner-image{
        position: relative;
        text-align: center;
          width:100%;
        display: block;
        -webkit-transition: all 0.25s cubic-bezier(0.37, 0.8, 0.76, 0.96);
        -o-transition: all 0.25s cubic-bezier(0.37, 0.8, 0.76, 0.96);
        transition: all 0.25s cubic-bezier(0.37, 0.8, 0.76, 0.96);
        }
        &:hover{
          .banner-image{
            -webkit-transform: scale(1.1);
            -ms-transform: scale(1.1);
            -o-transform: scale(1.1);
            transform: scale(1.1);
          }
          .banner-content{
             -webkit-transform: scale(1);
            -ms-transform: scale(1);
            -o-transform: scale(1);
            transform: scale(1);
          }

        }

        
        .banner-content{
          width: 100%;
          position: absolute;
          top: 35%;
          left: 0%;
          z-index: 10;
        	text-align:center;
          h5.white {
            color: #fff;
            line-height: 1.4;
            margin: 0 0 20px;
            text-transform: uppercase;
            font-weight: bold;
            letter-spacing: 0em;
            font-size: 18px;
          }
          h3.white {
            color: #fff;
            line-height: 1.4;
            margin: 0 0 20px;
            text-transform: uppercase;
            font-weight: bold;
            letter-spacing: 0em;
            font-size: 30px;
          }
          a.btn.btn-light-b {
            background: none;
            border: 1px solid #fff;
            border-radius: 0;
            padding: 10px 15px;
            color: #fff;
            &:hover{
              background:#ffa200;
              color:#fff;
              border: 1px solid #ffa200;
            }
          }
        }
      }
    }
    @media (max-width: 991px){
